{
  "name": "feeder",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev src/bootstrap.ts",
    "build": "tsc",
    "build-client": "NODE_ENV=production ts-node src/client-build.ts src/app/index/hydrate.tsx",
    "start": "NODE_ENV=production node dist/bootstrap.js",
    "lint": "eslint src --ext ts,tsx",
    "check": "npm run lint && npm run test && tsc --noEmit",
    "clean": "node -e 'fs.rmSync(`dist`, {recursive: true, force: true})'",
    "prebuild": "npm run clean",
    "postbuild": "npm run build-client",
    "test": "node --require ts-node/register --test src/tests/*"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dayjs": "^1.10.7",
    "fastify": "^3.27.0",
    "fastify-helmet": "^7.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "esbuild": "^0.19.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
